        <xsd:element ref="biz:lockTimeSt" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:businessKey" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:arbitraryKey" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:resendFlag" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:phase" maxOccurs="1" minOccurs="0"></xsd:element>
        </xsd:all>
    </xsd:complexType>

    <xsd:element name="appName">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="40"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="svcName">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="256"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="fnName">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="32"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="fnCd">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="2"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="globalNo">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="32"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="chnlType">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="2"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="envrFlag">
        <xsd:simpleType>
