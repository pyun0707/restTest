            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="trDate">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="8"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="trTime">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="20"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="clntIp">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="31"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseType">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="1"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseCode">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="20"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseLogcd">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="20"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseTitle">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="50"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseBasc">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="100"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseDtal">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="500"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="responseSystem">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="20"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="userId">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="12"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="realUserId">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="12"></xsd:maxLength>
            </xsd:restriction>
