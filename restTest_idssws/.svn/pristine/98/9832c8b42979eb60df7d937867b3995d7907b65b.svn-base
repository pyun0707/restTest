<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://bizhub.kt.com" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:biz="http://bizhub.kt.com">

    <xsd:complexType name="commonHeader">
        <xsd:all>
        <xsd:element ref="biz:appName" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:svcName" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:fnName" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:fnCd" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:globalNo" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:chnlType" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:envrFlag" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:trFlag" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:trDate" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:trTime" maxOccurs="1" minOccurs="1"></xsd:element>
