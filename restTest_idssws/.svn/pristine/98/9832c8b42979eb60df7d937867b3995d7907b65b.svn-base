        <xsd:element ref="biz:realUserId" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:filler" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:langCode" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:orgId" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:srcId" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:curHostId" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:lgDateTime" maxOccurs="1" minOccurs="1"></xsd:element>
