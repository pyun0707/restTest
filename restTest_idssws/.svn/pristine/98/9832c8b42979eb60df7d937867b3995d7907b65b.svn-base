    <xsd:element name="cbSvcName">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="40"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
    <xsd:element name="cbFnName">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:maxLength value="32"></xsd:maxLength>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>

</xsd:schema>