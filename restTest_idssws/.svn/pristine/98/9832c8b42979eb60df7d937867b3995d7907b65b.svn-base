        <xsd:element ref="biz:orderId" maxOccurs="1" minOccurs="0"></xsd:element>
        <xsd:element ref="biz:cbSvcName" maxOccurs="1" minOccurs="1"></xsd:element>
        <xsd:element ref="biz:cbFnName" maxOccurs="1" minOccurs="1"></xsd:element>
        </xsd:all>
